  "query": "\n                SELECT\n                    id,\n                    requestor_id, \n                    recipient_id, \n                    created_at, \n                    introduction_message, \n                    status as \"status: FriendRequestStatus\"\n                FROM friend_request\n                WHERE (recipient_id = $1 AND $2 = 'incoming') or (requestor_id = $1 AND $2 = 'outgoing') AND status = 'pending'\n            ",
