generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:database"
}

model ScheduledEntity {
    id          String   @id @default(uuid())
    startTime   DateTime
    endTime     DateTime
    category    String
    description String?
}

model RecordedEntity {
    id          String   @id @default(uuid())
    startTime   DateTime
    category    String
    description String?
}
